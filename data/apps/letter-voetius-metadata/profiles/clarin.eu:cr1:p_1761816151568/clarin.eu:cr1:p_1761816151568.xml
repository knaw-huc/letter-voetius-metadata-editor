<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ComponentSpec isProfile="true" CMDVersion="1.2" CMDOriginalVersion="1.2" xsi:noNamespaceSchemaLocation="https://infra.clarin.eu/CMDI/1.x/xsd/cmd-component.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <Header>
        <ID>clarin.eu:cr1:p_1761816151568</ID>
        <Name>Place</Name>
        <Description>For place names which are confirmed to exist.</Description>
        <Status>development</Status>
    </Header>
    <Component name="Place" CardinalityMin="1" CardinalityMax="1">
        <Element name="placeAppellation" ConceptLink="http://www.cidoc-crm.org/cidoc-crm/E44_Place_Appellation" ValueScheme="string" CardinalityMin="1" CardinalityMax="1" Multilingual="true" cue:DisplayPriority="1" xmlns:cue="http://www.clarin.eu/cmd/cues/1"/>
        <Component name="Identifier" ComponentRef="clarin.eu:cr1:c_1761816151559" ConceptLink="http://purl.org/dc/terms/identifier" CardinalityMin="1" CardinalityMax="1">
            <Documentation>A unique identifier of the resource being described or cataloged. Usage note: The identifier is a text string which is assigned to the resource to provide an unambiguous reference within a particular context. The identifier property represents any kind of identifier for any kind of Thing, such as ISBNs, GTIN codes, UUIDs etc. </Documentation>
            <Element name="identifierType" CardinalityMin="1" CardinalityMax="1">
                <ValueScheme>
                    <Vocabulary>
                        <enumeration>
                            <item ConceptLink="http://id.loc.gov/ontologies/bibframe/Local" AppInfo="">Local-identifier</item>
                            <item ConceptLink="" AppInfo="">URI</item>
                        </enumeration>
                    </Vocabulary>
                </ValueScheme>
            </Element>
            <Element name="identifierLabel" ConceptLink="http://purl.org/dc/terms/identifier" ValueScheme="string" CardinalityMin="1" CardinalityMax="1" cue:DisplayPriority="1" xmlns:cue="http://www.clarin.eu/cmd/cues/1">
                <Documentation>Add the preferred label for this identifier.</Documentation>
            </Element>
            <Element name="identifierUrl" ConceptLink="http://purl.org/dc/terms/identifier" ValueScheme="anyURI" CardinalityMin="1" CardinalityMax="1" cue:DisplayPriority="2" xmlns:cue="http://www.clarin.eu/cmd/cues/1">
                <Documentation>The complete URL if the identifier has a web locator. If it is OpenStreetMaps: Enter the URI from OpenStreetMap API. You can search directly in OpenStreetMaps and paste the URI here. The URI has to have this format, for example: https://www.openstreetmap.org/relation/214885 (note the type in the middle: relation, node, way)</Documentation>
            </Element>
            <Element name="schemeAgency" ConceptLink="http://www.w3.org/ns/adms#schemeAgency" CardinalityMin="1" CardinalityMax="1" cue:DisplayPriority="3" xmlns:cue="http://www.clarin.eu/cmd/cues/1">
                <Documentation>The name of the external source/knowledge base used to get the identifier. </Documentation>
                <ValueScheme>
                    <Vocabulary>
                        <enumeration>
                            <item ConceptLink="https://viaf.org" AppInfo="Virtual International Authority File">VIAF</item>
                            <item ConceptLink="https://www.wikidata.org" AppInfo="">Wikidata</item>
                            <item ConceptLink="https://www.openstreetmap.org/relation" AppInfo="">OpenStreetMaps</item>
                            <item ConceptLink="" AppInfo="">EMLO</item>
                            <item ConceptLink="" AppInfo="">CERL</item>
                            <item ConceptLink="" AppInfo="">NODEGOAT</item>
                        </enumeration>
                    </Vocabulary>
                </ValueScheme>
            </Element>
        </Component>
    </Component>
</ComponentSpec>
